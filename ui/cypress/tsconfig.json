{
  "compilerOptions": {
    "baseUrl": "/",
    "target": "es2022",
    "module": "ES2022",
    "moduleResolution": "node",
    "lib": ["es2022", "dom"],
    "types": ["cypress", "node"],
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
  },
  "include": ["**/*.ts"]
}
